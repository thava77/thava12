Thavaneshwaran Murugan:
// Define the pin numbers for the traffic lights and buttons
const int S1 = 2; // Green light on Warner St
const int S2 = 3; // Green light on 1st St
const int Y1 = 4; // Yellow light on Warner St
const int Y2 = 5; // Yellow light on 1st St
const int R1 = 6; // Red light on Warner St
const int R2 = 7; // Red light on 1st St
const int B1 = 8; // Walk button on Warner St
const int B2 = 9; // Walk button on 1st St
const int W1 = 10; // Walk signal on Warner St
const int W2 = 11; // Walk signal on 1st St
const int SW1 = 12; // Sensor on Warner St
const int SW2 = 13; // Sensor on 1st St

// Define the timings for the traffic lights
const int greenTime = 7000; // 7 seconds for green light
const int yellowTime = 2000; // 2 seconds for yellow light
const int walkTime = 4000; // 4 seconds for walk signal

void setup() {
  // Set the pins as input or output
  pinMode(S1, OUTPUT);
  pinMode(S2, OUTPUT);
  pinMode(Y1, OUTPUT);
  pinMode(Y2, OUTPUT);
  pinMode(R1, OUTPUT);
  pinMode(R2, OUTPUT);
  pinMode(B1, INPUT_PULLUP);
  pinMode(B2, INPUT_PULLUP);
  pinMode(W1, OUTPUT);
  pinMode(W2, OUTPUT);
  pinMode(SW1, INPUT_PULLUP);
  pinMode(SW2, INPUT_PULLUP);

  // Initialize the traffic lights
  digitalWrite(S1, LOW);
  digitalWrite(S2, LOW);
  digitalWrite(Y1, LOW);
  digitalWrite(Y2, LOW);
  digitalWrite(R1, HIGH);
  digitalWrite(R2, HIGH);
  digitalWrite(W1, LOW);
  digitalWrite(W2, LOW);
}

void loop() {
  // Check the state of the buttons and sensors
  int sw1 = digitalRead(SW1);
  int sw2 = digitalRead(SW2);
  int b1 = digitalRead(B1);
  int b2 = digitalRead(B2);

  if (sw1 == HIGH && sw2 == LOW) { // Traffic on Warner St, no traffic on 1st St
    digitalWrite(S1, HIGH);
    digitalWrite(R1, LOW);
    digitalWrite(Y1, LOW);
    delay(greenTime);
    digitalWrite(Y1, HIGH);
    delay(yellowTime);
    digitalWrite(R1, HIGH);
    digitalWrite(Y1, LOW);
    digitalWrite(S2, HIGH);
    digitalWrite(R2, LOW);
    delay(greenTime);
    digitalWrite(Y2, HIGH);
    delay(yellowTime);
    digitalWrite(R2, HIGH);
    digitalWrite(Y2, LOW);
  }
  else if (sw1 == LOW && sw2 == HIGH) { // Traffic on 1st St, no traffic on Warner St
    digitalWrite(S2, HIGH);
    digitalWrite(R2, LOW);
    digitalWrite(Y2, LOW);
    delay(greenTime);
    digitalWrite(Y2, HIGH);
    delay(yellowTime);
    digitalWrite(R2, HIGH);
    digitalWrite(Y2, LOW);
    digitalWrite(S1, HIGH);
    digitalWrite(R1, LOW);
    delay(greenTime); 
    digitalWrite(Y1, HIGH);
    delay(yellowTime);
    digitalWrite(R1, HIGH);
    digitalWrite(Y1, LOW);
  }

  else if (sw1 == LOW && sw2 == LOW) { // No traffic on any road, priority to Warner St
    digitalWrite(S2, HIGH);
    digitalWrite(R2, LOW);
    digitalWrite(Y2, LOW);
    delay(greenTime);
    digitalWrite(Y2, HIGH);
    delay(yellowTime);
    digitalWrite(R2, HIGH);
    digitalWrite(Y2, LOW);
    digitalWrite(S1, HIGH);
    digitalWrite(R1, LOW);
    delay(greenTime);
    digitalWrite(Y1, HIGH);
    delay(yellowTime);
    digitalWrite(R1, HIGH);
    digitalWrite(Y1, LOW);
  }
  else { // Cars coming in all directions
    digitalWrite(S1, HIGH);
    digitalWrite(R1, LOW);
    digitalWrite(Y1, LOW);
    delay(greenTime);
    digitalWrite(Y1, HIGH);
    delay(yellowTime);
    digitalWrite(R1, HIGH);
    digitalWrite(Y1, LOW);
    digitalWrite(S2, HIGH);
    digitalWrite(R2, LOW);
    delay(greenTime);
    digitalWrite(Y2, HIGH);
    delay(yellowTime);
    digitalWrite(R2, HIGH);
    digitalWrite(Y2, LOW);
    digitalWrite(S1, HIGH);
    digitalWrite(R1, LOW);
    delay(greenTime);
    digitalWrite(Y1, HIGH);
    delay(yellowTime);
    digitalWrite(R1, HIGH);
    digitalWrite(Y1, LOW);
    digitalWrite(S2, HIGH);
    digitalWrite(R2, LOW);
    delay(greenTime);
    digitalWrite(Y2, HIGH);
    delay(yellowTime);
    digitalWrite(R2, HIGH);
    digitalWrite(Y2, LOW);
  }

if (b1 == LOW && b2 == LOW) { // No walk requests
    digitalWrite(W1, LOW);
    digitalWrite(W2, LOW);
  }
  else if (b1 == HIGH && b2 == LOW) { // Walk request on Warner St
    digitalWrite(S1, LOW);
    digitalWrite(R1, HIGH);
    digitalWrite(Y1, HIGH);
    digitalWrite(S2, LOW);
    digitalWrite(R2, HIGH);
    digitalWrite(Y2, HIGH);
    delay(walkTime);
    digitalWrite(W1, HIGH);
    digitalWrite(Y1, LOW);
    delay(yellowTime);
    digitalWrite(W1, LOW);
    digitalWrite(R1, LOW);
    digitalWrite(R2, LOW);
    digitalWrite(Y2, LOW);
  }
  else if (b1 == LOW && b2 == HIGH) { // Walk request on 1st St
    digitalWrite(S2, LOW);
    digitalWrite(R2, HIGH);
    digitalWrite(Y2, HIGH);
    digitalWrite(S1, LOW);
    digitalWrite(R1, HIGH);
    digitalWrite(Y1, HIGH);
    delay(walkTime);
    digitalWrite(W2, HIGH);
    digitalWrite(Y2, LOW);
    delay(yellowTime);
    digitalWrite(W2, LOW);
    digitalWrite(R2, LOW);
    digitalWrite(R1, LOW);
    digitalWrite(Y1, LOW);
  }
  else { // Walk requests on both streets
    digitalWrite(S1, LOW);
    digitalWrite(R1, HIGH);
    digitalWrite(Y1, HIGH);
    digitalWrite(S2, LOW);
    digitalWrite(R2, HIGH);
    digitalWrite(Y2, HIGH);
    delay(walkTime);
    digitalWrite(W1, HIGH);
    digitalWrite(W2, HIGH);
    digitalWrite(Y1, LOW);
    digitalWrite(Y2, LOW);
    delay(yellowTime);
    digitalWrite(W1, LOW);
    digitalWrite(W2, LOW);
    digitalWrite(R1, LOW);
    digitalWrite(R2, LOW);
  }
}
